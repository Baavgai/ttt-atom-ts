{
  "name": "jotai-tic-tac",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "shx rm -r --silent ./dist || shx true",
    "prebuild": "npm run clean",
    "build": "parcel build public/index.html --no-source-maps",
    "start": "parcel public/index.html --open",
    "tsc": "npx tsc --noEmit",
    "tsc:watch": "npx tsc  --noEmit --watch",
    "lint:fix": "npx eslint -c ./.eslintrc.json --fix src/**/*.{ts,tsx}",
    "lint": "npx eslint -c ./.eslintrc.json src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "jotai": "^1.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-plugin-react": "^7.31.1",
    "parcel-bundler": "^1.12.5",
    "sass": "^1.54.7",
    "shx": "^0.3.4",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  }
}
